{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "cli": {
        "analytics": "9ec53a26-f7b9-4476-a768-970d94535e4f",
        "cache": {
            "enabled": false
        }
    },
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "resort-realestate-front": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                },
                "@schematics/angular:application": {
                    "strict": true
                }
            },
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/resort-realestate-front",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            
                            "src/assets/css/feather-icon.css",
                            "node_modules/leaflet/dist/leaflet.css",
                            "src/styles.scss",
                            "node_modules/tabulator-tables/dist/css/tabulator.css",
                            "node_modules/@ng-select/ng-select/themes/default.theme.css",
                            "node_modules/ngx-spinner/animations/ball-scale-multiple.css",
                            "node_modules/quill/dist/quill.core.css",
                            "node_modules/quill/dist/quill.bubble.css",
                            "node_modules/quill/dist/quill.snow.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeng/resources/themes/lara-light-blue/theme.css",
                            "node_modules/primeng/resources/primeng.min.css",
                            "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.carousel.min.css",
                            "node_modules/ngx-owl-carousel-o/lib/styles/prebuilt-themes/owl.theme.default.min.css"
                        
                        ],
                        "scripts": [
                            "src/assets/js/jquery/3.5.1/jquery.min.js",
                            "src/assets/js/popper/1.16.0/popper.min.js",
                            "src/assets/scss/vendors/Blast-Color-Changer/jquery-1.11.3.js",
                            "src/assets/scss/vendors/Blast-Color-Changer/blast.min.js",
                            "src/assets/js/loader.js",
                            "src/assets/js/main.js",
                            "src/assets/website/js/popper.min.js",
                            "node_modules/jquery/dist/jquery.js",
                            "src/assets/sniper/js/popper.min.js",
                            "src/assets/js/bootstrap/4.5.2/bootstrap.min.js",
                            "node_modules/owl.carousel/dist/owl.carousel.min.js",
                            "src/assets/website/js/main.js",
                            "src/assets/website/js/owl-carousels.js",
                            "src/assets/website/js/browser-class.js",
                            "node_modules/quill/dist/quill.js",
                            "src/assets/exportJs/xlsx.full.min.js",
                            "src/assets/exportJs/jspdf.min.js",
                            "src/assets/exportJs/jspdf.plugin.autotable.js"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "5mb",
                                    "maximumError": "6mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "5mb",
                                    "maximumError": "6mb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "resort-realestate-front:build:production"
                        },
                        "development": {
                            "browserTarget": "resort-realestate-front:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "resort-realestate-front:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "tsconfig.spec.json",
                        "karmaConfig": "karma.conf.js",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "defaultProject": "resort-realestate-front"
}